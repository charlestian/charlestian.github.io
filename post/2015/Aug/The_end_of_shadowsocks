      <script>
        $(function() {
          var page_id = '/post/2015/Aug/The_end_of_shadowsocks.html'.trim();
          var page_url = '/post/2015/Aug/The_end_of_shadowsocks.html'.trim();
          var id = '';
          if (page_id == '') {
              id = page_url.replace('.html', '');
          } else {
              id = page_id;
          }

          renderComments($, id);

          $('#submit_comment').submit(function(e) {
              e.preventDefault();
              submitComment($, id);
          });
        });
      </script>


